create table kmedymember(
usernum number not null,
userid varchar2(200) primary key, -- 아이디
passwd varchar2(200) not null, -- 비밀번호
username varchar2(200) not null, -- 이름
useremail varchar2(200) not null, -- 이메일
phone varchar2(200) not null, -- 핸드폰
birthday varchar2(200) not null, -- 생년월일
postcode varchar2(200) not null, --우편번호
address varchar2(200) not null, --주소
address2 varchar2(200) not null, --상세 주소
join_date date default sysdate, -- 가입날짜
point number default 0 -- 포인트
);
